{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "strict": "off",
        "space-before-blocks": "off",
        "indent": "off",
        "lines-between-class-members": "off",
        "import/prefer-default-export": "off",
        "no-plusplus": "off",
        "arrow-parens": "off",
        "class-methods-use-this": "off",
        "radix": "off",
        "max-len": "off",
        "import/extensions": "off"
    }
}